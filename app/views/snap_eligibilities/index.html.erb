<h1>Listing snap_eligibilities</h1>

<table>
  <thead>
    <tr>
      <th>Snap dependent no</th>
      <th>Snap gross income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @snap_eligibilities.each do |snap_eligibility| %>
      <tr>
        <td><%= snap_eligibility.snap_dependent_no %></td>
        <td><%= snap_eligibility.snap_gross_income %></td>
        <td><%= link_to 'Show', snap_eligibility %></td>
        <td><%= link_to 'Edit', edit_snap_eligibility_path(snap_eligibility) %></td>
        <td><%= link_to 'Destroy', snap_eligibility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snap eligibility', new_snap_eligibility_path %>
