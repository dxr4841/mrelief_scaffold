%h1 Listing snap_eligibilities
%table
  %thead
    %tr
      %th Snap dependent no
      %th Snap gross income
      %th{:colspan => "3"}
  %tbody
    - @snap_eligibilities.each do |snap_eligibility|
      %tr
        %td= snap_eligibility.snap_dependent_no
        %td= snap_eligibility.snap_gross_income
        %td= link_to 'Show', snap_eligibility
        %td= link_to 'Edit', edit_snap_eligibility_path(snap_eligibility)
        %td= link_to 'Destroy', snap_eligibility, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Snap eligibility', new_snap_eligibility_path
