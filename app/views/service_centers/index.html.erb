<h1>Listing service_centers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_centers.each do |service_center| %>
      <tr>
        <td><%= service_center.name %></td>
        <td><%= service_center.latitude %></td>
        <td><%= service_center.longitude %></td>
        <td><%= service_center.street %></td>
        <td><%= service_center.city %></td>
        <td><%= service_center.state %></td>
        <td><%= service_center.zip %></td>
        <td><%= service_center.phone %></td>
        <td><%= service_center.organization %></td>
        <td><%= link_to 'Show', service_center %></td>
        <td><%= link_to 'Edit', edit_service_center_path(service_center) %></td>
        <td><%= link_to 'Destroy', service_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service center', new_service_center_path %>
