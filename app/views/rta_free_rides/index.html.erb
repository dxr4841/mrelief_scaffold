<h1>Listing rta_free_rides</h1>

<table>
  <thead>
    <tr>
      <th>Rta dependent no</th>
      <th>Rta gross income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rta_free_rides.each do |rta_free_ride| %>
      <tr>
        <td><%= rta_free_ride.rta_dependent_no %></td>
        <td><%= rta_free_ride.rta_gross_income %></td>
        <td><%= link_to 'Show', rta_free_ride %></td>
        <td><%= link_to 'Edit', edit_rta_free_ride_path(rta_free_ride) %></td>
        <td><%= link_to 'Destroy', rta_free_ride, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rta free ride', new_rta_free_ride_path %>
