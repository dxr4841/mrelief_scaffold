<h1>Listing medicare_cost_sharings</h1>

<table>
  <thead>
    <tr>
      <th>Household size</th>
      <th>Medicare cost sharing</th>
      <th>Premium only</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicare_cost_sharings.each do |medicare_cost_sharing| %>
      <tr>
        <td><%= medicare_cost_sharing.household_size %></td>
        <td><%= medicare_cost_sharing.medicare_cost_sharing %></td>
        <td><%= medicare_cost_sharing.premium_only %></td>
        <td><%= link_to 'Show', medicare_cost_sharing %></td>
        <td><%= link_to 'Edit', edit_medicare_cost_sharing_path(medicare_cost_sharing) %></td>
        <td><%= link_to 'Destroy', medicare_cost_sharing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medicare cost sharing', new_medicare_cost_sharing_path %>
