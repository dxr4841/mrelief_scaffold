<h1>Listing rental_assistances</h1>

<table>
  <thead>
    <tr>
      <th>Rental dependent no</th>
      <th>Rental gross income</th>
      <th>Rental status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rental_assistances.each do |rental_assistance| %>
      <tr>
        <td><%= rental_assistance.rental_dependent_no %></td>
        <td><%= rental_assistance.rental_gross_income %></td>
        <td><%= rental_assistance.rental_status %></td>
        <td><%= link_to 'Show', rental_assistance %></td>
        <td><%= link_to 'Edit', edit_rental_assistance_path(rental_assistance) %></td>
        <td><%= link_to 'Destroy', rental_assistance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rental assistance', new_rental_assistance_path %>
