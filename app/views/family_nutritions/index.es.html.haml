%h1 Listing family_nutritions
%table
  %thead
    %tr
      %th Nutrition gross income
      %th Nutrition household size
      %th{:colspan => "3"}
  %tbody
    - @family_nutritions.each do |family_nutrition|
      %tr
        %td= family_nutrition.nutrition_gross_income
        %td= family_nutrition.nutrition_household_size
        %td= link_to 'Show', family_nutrition
        %td= link_to 'Edit', edit_family_nutrition_path(family_nutrition)
        %td= link_to 'Destroy', family_nutrition, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Family nutrition', new_family_nutrition_path
