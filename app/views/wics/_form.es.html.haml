= form_for(@wic) do |f|
  - if @wic.errors.any?
    #error_explanation
      %h2
        = pluralize(@wic.errors.count, "error")
        prohibited this wic from being saved:
      %ul
        - @wic.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :wic_household_size
    %br/
    = f.number_field :wic_household_size
  .field
    = f.label :wic_gross_income
    %br/
    = f.text_field :wic_gross_income
  .actions
    = f.submit
