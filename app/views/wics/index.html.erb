<h1>Listing wics</h1>

<table>
  <thead>
    <tr>
      <th>Wic household size</th>
      <th>Wic gross income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wics.each do |wic| %>
      <tr>
        <td><%= wic.wic_household_size %></td>
        <td><%= wic.wic_gross_income %></td>
        <td><%= link_to 'Show', wic %></td>
        <td><%= link_to 'Edit', edit_wic_path(wic) %></td>
        <td><%= link_to 'Destroy', wic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wic', new_wic_path %>
