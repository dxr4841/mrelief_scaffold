= form_for(@housing_assistance) do |f|
  - if @housing_assistance.errors.any?
    #error_explanation
      %h2
        = pluralize(@housing_assistance.errors.count, "error")
        prohibited this housing_assistance from being saved:
      %ul
        - @housing_assistance.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :housing_dependent_no
    %br/
    = f.number_field :housing_dependent_no
  .field
    = f.label :housing_gross_income
    %br/
    = f.text_field :housing_gross_income
  .actions
    = f.submit
